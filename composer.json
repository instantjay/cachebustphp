{
    "name": "instantjay/cachebustphp",
    "description": "Small library for cache busting your hyperlinked resources",
    "type": "library",
    "require": {
        "php": ">5.6",
        "symfony/filesystem": "^4.1",
        "symfony/finder": "^4.1",
        "symfony/http-foundation": "^4.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.3",
        "squizlabs/php_codesniffer": "^3.3",
        "mockery/mockery": "^1.1"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "John Jensen",
            "email": "john@jenseninnovations.se"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    }
}
