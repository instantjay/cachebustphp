{
    "name": "instantjay/cachebustphp",
    "description": "Small library for cache busting your hyperlinked resources",
    "type": "library",
    "require": {
        "symfony/filesystem": "^5.0",
        "symfony/finder": "^5.0",
        "symfony/http-foundation": "^5.0",
        "php": "^7.1"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.5",
        "mockery/mockery": "^1.3",
        "phpunit/phpunit": "^9.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "John Jensen",
            "email": "john@jenseninnovations.se"
        }
    ],
    "autoload": {
        "psr-4": {
            "instantjay\\cachebustphp\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "instantjay\\cachebustphp\\Tests\\": "tests/"
        }
    }
}
